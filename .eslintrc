{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "google",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"one-var": 0,
		"one-var-declaration-per-line": 2,
		"no-invalid-this": 0,
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"no-trailing-spaces": "off",
		"max-len": "off",
		"prefer-rest-params": "off",
    "indent": "off"
	}
}
